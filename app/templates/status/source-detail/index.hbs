
<ul class="breadcrumb">
	<li>{{#link-to "status.index"}} Status-Board {{/link-to}}</li>
  <li>{{model.id}}</li>
</ul>

<div>
	<table class= "logListTable">
		<tr>
			<th>STATUS</th>
			<th>CONTEXT</th>
			<th>HARVEST LOG ID</th>
			<th>DATE STARTED</th>
			<th>DATE ENDED</th>
			<th>SHARE VERSION</th>
		</tr>
	{{#each model as |hl|}}
		<tr>
			<td>
				{{#link-to "status.source-detail.log-detail" hl}}
				{{#status-emoji status=hl.status}}
				{{/status-emoji}}
				{{/link-to}}
			</td>

		<td>{{hl.status}}</td>
		<td>{{hl.id}}</td>
		<td>{{hl.startDate}}</td>
		<td>{{hl.endDate}}</td>
		<td>{{hl.harvesterVersion}}</td>
	</tr>
	{{/each}}
	</table>
</div>
